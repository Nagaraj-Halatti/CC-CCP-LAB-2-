#include<stdio.h>
#include<conio.h>
int main()
{
	int a,b;
    printf("enter the values  ");	
    scanf("%d%d",&a,&b);
    if(a>b)
    printf("%d is greater than %d",a,b);
    if(a<b)
    printf("%d is greater than %d",b,a );
    if(a==b)
    printf("\n:%d is equal to %d",a,b);
    getch();
    return 0;
}


